{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Memory",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "${workspaceFolder}/memoria/memoria.go",
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen"
    },
    {
      "name": "Launch CPU",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "${workspaceFolder}/cpu/cpu.go",
      "cwd": "${workspaceFolder}",
      "args": ["cpuX"],
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen"
    }
  ]
}